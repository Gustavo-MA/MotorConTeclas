    {
        "id": "c73a161767920460",
        "type": "ui_template",
        "z": "cc23c93d429a02c4",
        "d": true,
        "group": "8b51f80ad55c39e6",
        "name": "",
        "order": 2,
        "width": 0,
        "height": 0,
        "format": "<div id=\"scene3d\" style=\"width:100%; height:400px; background:#111;\"></div>\n\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js\"></script>\n\n<script>\n    (function() {\n\n    let scene, camera, renderer;\n    let base, grupo1, grupo2, grupo3;\n    let brazo1, brazo2, brazo3;\n\n    // Esperar 200ms para que el dashboard dibuje el div\n    setTimeout(init3D, 200);\n\n    // ======================\n    // ESCUCHAR MENSAJES\n    // ======================\n    window.addEventListener(\"message\", function(evt) {\n        let msg = evt.data;\n\n        if (!msg || !msg.payload) return;\n\n        if (msg.payload.contador1 !== undefined) {\n            grupo1.rotation.z = THREE.Math.degToRad(msg.payload.contador1 - 90);\n        }\n        if (msg.payload.contador2 !== undefined) {\n            grupo2.rotation.z = THREE.Math.degToRad(msg.payload.contador2 - 90);\n        }\n        if (msg.payload.contador3 !== undefined) {\n            grupo3.rotation.z = THREE.Math.degToRad(msg.payload.contador3 - 20);\n        }\n    });\n\n\n    // ======================\n    // INICIALIZACIÓN 3D\n    // ======================\n    function init3D() {\n\n        const container = document.getElementById(\"scene3d\");\n        if (!container) return;\n\n        scene = new THREE.Scene();\n\n        camera = new THREE.PerspectiveCamera(\n            50,\n            container.offsetWidth / container.offsetHeight,\n            0.1,\n            1000\n        );\n\n        camera.position.set(0, -8, 5);\n        camera.lookAt(0, 0, 0);\n\n        renderer = new THREE.WebGLRenderer({ antialias: true });\n        renderer.setSize(container.offsetWidth, container.offsetHeight);\n        container.appendChild(renderer.domElement);\n\n        // Luz\n        const light = new THREE.PointLight(0xffffff, 1.4);\n        light.position.set(6, 6, 8);\n        scene.add(light);\n\n        const ambient = new THREE.AmbientLight(0x404040);\n        scene.add(ambient);\n\n        // ======= BASE =======\n        base = new THREE.Mesh(\n            new THREE.CylinderGeometry(0.7, 0.7, 0.4, 32),\n            new THREE.MeshStandardMaterial({ color: 0x333333 })\n        );\n        scene.add(base);\n\n        // ===== GRUPO 1 =====\n        grupo1 = new THREE.Group();\n        grupo1.position.set(0, 0.2, 0);\n        base.add(grupo1);\n\n        brazo1 = crearSegmento(2.0, 0.4, 0xFF9933);\n        brazo1.position.y = 1.0;\n        grupo1.add(brazo1);\n\n        let joint1 = crearArticulacion();\n        joint1.position.y = 2.0;\n        grupo1.add(joint1);\n\n        // ===== GRUPO 2 =====\n        grupo2 = new THREE.Group();\n        grupo2.position.y = 2.0;\n        grupo1.add(grupo2);\n\n        brazo2 = crearSegmento(2.0, 0.35, 0x33AAFF);\n        brazo2.position.y = 1.0;\n        grupo2.add(brazo2);\n\n        let joint2 = crearArticulacion();\n        joint2.position.y = 2.0;\n        grupo2.add(joint2);\n\n        // ===== GRUPO 3 =====\n        grupo3 = new THREE.Group();\n        grupo3.position.y = 2.0;\n        grupo2.add(grupo3);\n\n        brazo3 = crearSegmento(1.8, 0.3, 0x88FF88);\n        brazo3.position.y = 0.9;\n        grupo3.add(brazo3);\n\n        animate();\n    }\n\n\n    function crearSegmento(largo, grosor, color) {\n        return new THREE.Mesh(\n            new THREE.BoxGeometry(grosor, largo, grosor),\n            new THREE.MeshStandardMaterial({ color })\n        );\n    }\n\n    function crearArticulacion() {\n        return new THREE.Mesh(\n            new THREE.SphereGeometry(0.22, 32, 32),\n            new THREE.MeshStandardMaterial({ color: 0x222222 })\n        );\n    }\n\n\n    // ======================\n    // ANIMACIÓN\n    // ======================\n    function animate() {\n        requestAnimationFrame(animate);\n        if (renderer && scene && camera) {\n            renderer.render(scene, camera);\n        }\n    }\n\n})();\n</script>",
