        "format": "<script>\n    (function(scope) {\n\n    // Evitar múltiples listeners\n    if (window.top.__keyboardListenerAdded) return;\n    window.top.__keyboardListenerAdded = true;\n\n    (window.top || window).document.addEventListener(\"keydown\", function(e) {\n\n        // Evitar bloqueo por inputs\n        if ([\"INPUT\", \"TEXTAREA\"].includes(e.target.tagName)) return;\n\n        const key = e.key.toLowerCase();\n\n        if (key === \"a\") scope.send({ topic: \"topicM1-Izq\" });\n        if (key === \"d\") scope.send({ topic: \"topicM1-Der\" });\n\n        if (key === \"w\") scope.send({ topic: \"topicM2-Izq\" });\n        if (key === \"s\") scope.send({ topic: \"topicM2-Der\" });\n\n        if (key === \"o\") scope.send({ topic: \"topicM3-Der\" });\n        if (key === \"p\") scope.send({ topic: \"topicM3-Izq\" });\n\n        if (key === \"x\") scope.send({ topic: \"topicM4-Der\" });\n        if (key === \"z\") scope.send({ topic: \"topicM4-Izq\" });\n    });\n\n})(scope);\n</script>",
