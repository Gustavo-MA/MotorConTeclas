        "order": 2,
        "width": 0,
        "height": 0,
        "format": "<div id=\"scene3d\" style=\"width:100%; height:100%; min-height:400px; background:#111;\"></div>\n\n<script src=\"https://unpkg.com/three@0.128.0/build/three.min.js\"></script>\n\n<script>\n    (function(scope) {\n\n    function init3D() {\n        const div = document.getElementById(\"scene3d\");\n        const w = div.clientWidth || 400;\n        const h = div.clientHeight || 400;\n\n        // Configurar escena\n        const scene = new THREE.Scene();\n        const camera = new THREE.PerspectiveCamera(75, w/h, 0.1, 1000);\n        camera.position.z = 3;\n\n        const renderer = new THREE.WebGLRenderer({ antialias: true });\n        renderer.setSize(w, h);\n        div.appendChild(renderer.domElement);\n\n        // Cubo de prueba\n        const geometry = new THREE.BoxGeometry();\n        const material = new THREE.MeshStandardMaterial({ color: 0x00ff00 });\n        const cube = new THREE.Mesh(geometry, material);\n        scene.add(cube);\n\n        // Luz\n        const light = new THREE.PointLight(0xffffff, 1);\n        light.position.set(3, 3, 3);\n        scene.add(light);\n\n        // Ajuste si el dashboard cambia de tamaño\n        window.addEventListener(\"resize\", () => {\n            const w2 = div.clientWidth;\n            const h2 = div.clientHeight;\n            renderer.setSize(w2, h2);\n            camera.aspect = w2 / h2;\n            camera.updateProjectionMatrix();\n        });\n\n        // Animación\n        function animate() {\n            requestAnimationFrame(animate);\n            cube.rotation.x += 0.02;\n            cube.rotation.y += 0.03;\n            renderer.render(scene, camera);\n        }\n        animate();\n    }\n\n    // Esperar a que el dashboard termine de cargar\n    setTimeout(init3D, 300);\n\n})(scope);\n</script>",
